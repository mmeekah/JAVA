
public class FizzBuzz {

	public static void main(String[] args) {
		
		System.out.println(FizzBuzz(10));

	}

	public static String FizzBuzz (int num) {
		
		if ( num % 3 == 0) {
			System.out.println("Fizz");
		}
		
		if (num % 5 == 0) {
			System.out.println("Buzz");
			
		}
		if (num % 3 == 0 && num % 5 ==0) {
			System.out.println("FizzBuzz");
		}
		return null;
		
		
	}
}
